{
    "name": "@anime-autobattler/content",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./data/*": "./data/*"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "validate": "node --loader ts-node/esm src/validate.ts"
    },
    "dependencies": {
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    }
}